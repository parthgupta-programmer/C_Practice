#include <stdio.h>
#include <math.h>
//Array:To find minimum from the elements of array.
int main(){
    int i,n,min;
    printf("Enter the no. of elements:");
    scanf("%d",&n);
    int arr[n];
    for(i=0;i<=n-1;i++){
         scanf("%d",&arr[i]);
    }
    min=arr[0];
    for(i=0;i<=n-1;i++){
        if(arr[i]<min){
            min=arr[i];
        }
    }
    printf("Minimum Value:%d",min);
    return 0;
    
