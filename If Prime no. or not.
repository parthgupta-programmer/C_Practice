#include <stdio.h>
#include <math.h>
int main()
//To find if a number entered is a prime number or not.
{
    int n,i;
    printf("Enter any number:");
    scanf("%d",&n);
    for (i=2;i<=n-1;i++)
    {
         if (n%i==0){
             printf("Number entered is not a Prime No.");
             return 0;
         }
        
    }
    printf("Number entered is a Prime number.");
    return 0;
}
