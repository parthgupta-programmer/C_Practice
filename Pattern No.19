#include <stdio.h>
#include <math.h>
#include <stdlib.h>
#include <string.h>
int main(){//Pattern No.19
    int n=5;
    int c=(n*2)-2;
    int s=1;
    for(int i=1;i<=n;i++,c=c-2){
        for(int j=1;j<=c;j++){
            printf(" ");
        }
        for(int j=1;j<=(i*2)-1;j++){
            if(j==1 || j==(i*2)-1)
                 printf("1 ");
            else{
                printf("%d ",s);
            }
        }
        s++;
        printf("\n");
    }
    return 0;
}
