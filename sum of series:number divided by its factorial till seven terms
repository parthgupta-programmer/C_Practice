#include <stdio.h>
#include <math.h>
int main()
//To add first seven terms :1/1! + 2/2!+....
{
    int i,j;
    float f,s=0.0;
    for (i=1;i<=7;i++){
          f=1.0;
          for ( j=1;j<=i;j++)
                  f=f*j; 
                  s=s+i/f;
                       
    }
   
    printf("Sum:%f\n",s);
    return 0;
}
