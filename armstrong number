// Online C compiler to run C program online
#include <stdio.h>
#include <math.h>
// To print all armstrong numbers between 1 and 500
int main()
{
   int n,c,fd,sd,td;
   
   for (n=1;n<=500;n++)
   { int og=n;
       td=n%10;
       n=n/10;
       sd=n%10;
       n=n/10;
       fd=n;
       c=(pow(td,3)+pow(sd,3)+pow(fd,3));
       if (c==og) 
           printf("%d\n",n);
   }
    return 0;
}
